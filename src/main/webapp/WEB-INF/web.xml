<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>FoodTypeServlet</servlet-name>
        <servlet-class>sl.itcast.servlet.FoodType.FoodTypeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CuisineListServlet</servlet-name>
        <servlet-class>sl.itcast.servlet.FoodType.CuisineListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateCuisineServlet</servlet-name>
        <servlet-class>sl.itcast.servlet.FoodType.updateCuisineServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateServlet</servlet-name>
        <servlet-class>sl.itcast.servlet.FoodType.updateServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deletCuisineListServlet</servlet-name>
        <servlet-class>sl.itcast.servlet.FoodType.deletCuisineListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addCuisineServlet</servlet-name>
        <servlet-class>sl.itcast.servlet.FoodType.addCuisineServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchServlet</servlet-name>
        <servlet-class>sl.itcast.servlet.FoodType.SearchServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>foodServlet</servlet-name>
        <servlet-class>sl.itcast.servlet.Food.foodServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addFoodServlet</servlet-name>
        <servlet-class>sl.itcast.servlet.Food.addFoodServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addServlet</servlet-name>
        <servlet-class>sl.itcast.servlet.Food.addServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deletFoodServlet</servlet-name>
        <servlet-class>sl.itcast.servlet.Food.deletFoodServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updataFoodServlet</servlet-name>
        <servlet-class>sl.itcast.servlet.Food.updataFoodServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updataFood</servlet-name>
        <servlet-class>sl.itcast.servlet.Food.updataFood</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FoodSearchServlet</servlet-name>
        <servlet-class>sl.itcast.servlet.Food.FoodSearchServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FoodTypeServlet</servlet-name>
        <url-pattern>/FoodTypeServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CuisineListServlet</servlet-name>
        <url-pattern>/CuisineListServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateCuisineServlet</servlet-name>
        <url-pattern>/updateCuisineServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateServlet</servlet-name>
        <url-pattern>/updateServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deletCuisineListServlet</servlet-name>
        <url-pattern>/deletCuisineListServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addCuisineServlet</servlet-name>
        <url-pattern>/addCuisineServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchServlet</servlet-name>
        <url-pattern>/SearchServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>foodServlet</servlet-name>
        <url-pattern>/foodServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addFoodServlet</servlet-name>
        <url-pattern>/addFoodServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addServlet</servlet-name>
        <url-pattern>/addServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deletFoodServlet</servlet-name>
        <url-pattern>/deletFoodServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updataFoodServlet</servlet-name>
        <url-pattern>/updataFoodServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updataFood</servlet-name>
        <url-pattern>/updataFood</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FoodSearchServlet</servlet-name>
        <url-pattern>/FoodSearchServlet</url-pattern>
    </servlet-mapping>
</web-app>